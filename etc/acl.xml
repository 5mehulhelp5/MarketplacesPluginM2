<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="EffectConnect_Marketplaces::effectconnect" title="EffectConnect">
                    <resource id="EffectConnect_Marketplaces::marketplaces" title="EffectConnect Marketplaces">
                        <resource id="EffectConnect_Marketplaces::marketplaces_connections" title="Connections" sortOrder="0"/>
                        <resource id="EffectConnect_Marketplaces::marketplaces_channel_mapping" title="Channel mapping" sortOrder="1"/>
                        <resource id="EffectConnect_Marketplaces::marketplaces_logs" title="Logs" sortOrder="2"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="EffectConnect_Marketplaces::effectconnect_marketplaces" title="EffectConnect Marketplaces"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
	</acl>
</config>
